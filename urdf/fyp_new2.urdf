<?xml version="1.0"?>
<robot name="fyp_new" xmlns:xacro="http://ros.org/wiki/xacro">

  <!-- Model Dimensions -->
  <xacro:property name="pi" value="3.1415" />
  <xacro:property name="body_length" value="0.55" />
  <xacro:property name="body_width" value="0.16" />
